<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.DeptMapper">
    <insert id="insert" parameterType="com.example.pojo.Dept">
        insert into dept(name,create_time,update_time) values (#{name},#{createTime},#{updateTime});
    </insert>
    <update id="update" parameterType="com.example.pojo.Dept">
        update dept set name=#{name},create_time=#{createTime},update_time=#{updateTime} where id=#{id};
    </update>
    <select id="select" resultType="com.example.pojo.Dept">
        select *from dept where id=#{id};
    </select>
</mapper>
